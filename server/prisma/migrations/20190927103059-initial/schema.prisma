generator photon {
  provider = "photonjs"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Project {
  id      String @default(cuid()) @id
  title   String
  project Todo[]

}

model Todo {
  id       String   @default(cuid()) @id
  title    String
  complete Boolean
  project  Project?
}
